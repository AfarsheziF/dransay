{
  "name": "interview-prep-stack",
  "version": "1.0.0",
  "description": "Full-stack interview preparation project with Remix, tRPC, and Drizzle",
  "type": "module",
  "scripts": {
    "build": "npm run db:generate && npm run db:migrate && remix build",
    "dev": "cross-env NODE_OPTIONS=\"--no-deprecation\" remix dev --manual",
    "start": "remix-serve build/index.js",
    "typecheck": "tsc",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts"
  },
  "dependencies": {
    "@remix-run/node": "2.16.7",
    "@remix-run/react": "2.16.7",
    "@remix-run/serve": "2.16.7",
    "@remix-run/server-runtime": "2.16.7",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@vercel/remix": "2.16.7",
    "bcryptjs": "^2.4.3",
    "drizzle-kit": "^0.31.5",
    "drizzle-orm": "^0.44.6",
    "i": "^0.3.7",
    "isbot": "^5.1.31",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "npm": "^11.6.2",
    "pg": "^8.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.4",
    "remix-custom-routes": "^1.0.1",
    "superjson": "^2.2.1",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@remix-run/dev": "2.16.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.8.4",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.1.0",
    "esbuild": "^0.25.11",
    "postcss": "^8.5.6",
    "postgres": "^3.4.4",
    "tailwindcss": "^4.1.15",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vite": "^6.0.0"
  }
}
